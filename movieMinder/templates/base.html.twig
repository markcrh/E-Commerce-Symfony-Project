<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>
    {% block title %}
        Welcome!
    {% endblock %}
    </title>
    <link rel="icon" href="{{ asset('img/logoNoText.svg') }}">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        <link rel="stylesheet" href="{{ asset('styles/app.css') }}">
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {{ importmap() }}
    {% endblock %}
</head>
<body class="min-h-screen flex flex-col">
{% block body %}
    {% block header %}{% endblock %}
    {#<main class="flex flex-col flex-grow">
        {% block main %}{% endblock %}
    </main>#}
    {% block footer %}

    {% endblock %}
{% endblock %}

</body>
</html>
